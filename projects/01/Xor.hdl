/**
 * Exclusive-or gate:
 * out = (not(a) and b) or (a and not(b))
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=Nota);
    Not(in=b, out=Notb);
    Nand(a=Nota, b=b, out=Notab);
    Nand(a=a, b=Notb, out=aNotb);
    Nand(a=Notab, b=aNotb, out=out);
}
